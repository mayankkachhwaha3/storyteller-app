{
  "name": "storyteller",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.config.js --port 3002",
    "build": "vite build",
    "preview": "vite preview",
    "server": "tsx watch server/index.ts",
    "start": "node --experimental-json-modules --loader ts-node/esx server/index.ts",
    "dev:full": "concurrently -n 'vite,server' -c 'bgBlue.bold,bgGreen.bold' 'npm run dev' 'npm run server'"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "react": "^18",
    "react-dom": "^18",
    "react-router-dom": "^6.22.2"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10",
    "concurrently": "^8.2.2",
    "postcss": "^8",
    "tailwindcss": "^3",
    "ts-node": "^10.9.2",
    "tsx": "^4.6.2",
    "typescript": "^5.8.3",
    "vite": "^5"
  }
}
